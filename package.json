{
  "name": "youbora-adapter-html5",
  "version": "6.1.2",
  "main": "src/adapter.js",
  "description": "Youbora adapter for html5",
  "scripts": {
    "build": "npm run manifest && webpack -p",
    "watch": "npm run manifest && webpack -p --progress --color --watch",
    "deployable": "npm run build && youbora-util deployable",
    "manifest": "youbora-util manifest",
    "preversion": "npm run build",
    "clean": "rm -rf manifest.json dist deploy"
  },
  "dependencies": {
    "youboralib": "6.1.7"
  },
  "devDependencies": {
    "webpack": "^2.2.1",
    "youboralib-util": "1.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/npaw/html5-adapter-js.git"
  },
  "author": "Jordi Aguilar",
  "license": "MIT",
  "bugs": {
    "email": "support@nicepeopleatwork.com"
  }
}
